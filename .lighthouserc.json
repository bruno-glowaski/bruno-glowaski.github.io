{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000"
      ],
      "startServerCommand": "npx serve@latest out",
      "startServerReadyPattern": "started server on",
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
